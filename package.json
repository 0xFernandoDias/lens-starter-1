    "@ethersproject/providers": "^5.6.8",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@kitql/client": "^0.5.7",
    "@kitql/graphql-codegen": "^0.5.2",
    "@kitql/vite-plugin-watch-and-run": "^0.3.4",
    "@types/node": "^17.0.39",
    "daisyui": "^2.15.2",
    "ethers": "^5.6.8",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
